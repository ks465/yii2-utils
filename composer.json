{
  "name": "khans465/yii2-utils",
  "description": "A pack of multiple components and widgets, especially designed for simpler and more generic coding",
  "type": "yii2-extension",
  "keywords": [
    "yii2",
    "extension"
  ],
  "license": "GPL-2.0+",
  "authors": [
    {
      "name": "Keyhan Sedaghat",
      "email": "keyhansedaghat@netscape.net"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "yiisoft/yii2": "~2.0.0",
    "yiisoft/yii2-apidoc": "~2.1.0",
    "faravaghi/yii2-jalali-datepicker ": "~1.0",
    "kartik-v/yii2-grid": "~3.1.0",
    "johnitvn/yii2-ajaxcrud": "~2.1.0",
    "nhkey/yii2-activerecord-history": "*"
  },
  "autoload": {
    "psr-4": {
      "KHanS\\Utils\\": "src"
    },
    "classmap": [
      "src/components/",
      "src/columns/",
      "src/widgets/"
    ]
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.0.1-dev"
    }
  }
}
